<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jamorn.hibernate.entity">
    <class name="Site" table="m_site">
        <id name="id" type="java.lang.Long" column="id">
            <generator class="identity"/>
        </id>
        <property name="code" column="code" type="string" not-null="false" length="50"/>
        <property name="name" column="name" type="string" not-null="false" length="50"/>
        <property name="createTime" column="createtime" type="date" not-null="false" length="50"/>
    </class>
</hibernate-mapping>